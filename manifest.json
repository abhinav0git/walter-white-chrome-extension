{
    "name": "Letâ€™s cook this thing Jesse.",
    "version": "1.0",
    "manifest_version": 3,
    "description": "opens side panel, visible to all site",
    "background": {
        "service_worker": "service-worker.js"
    },
    "action" : {
        "default_title": "click to open panel"
    },
    "icons": {
        "16": "icons/icons1.png",
        "48": "icons/icons2.png",
        "128": "icons/icons3.png"
    },
    "commands" : {
        "_execute_action": {
            "suggested_key": {
                "default" : "Ctrl+Q",
                "mac" : "Command+Q"
            }
        }
    },
    "side_panel": {
        "default_path": "index.html"
    },
    "permissions": ["sidePanel", "tabs", "contextMenus", "storage"]
}
